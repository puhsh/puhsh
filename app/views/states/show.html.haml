- content_for :title do
  %title= "#{@current_state_name}, US"

- content_for :header do
  = render 'shared/header'

%section.row
  %article.small-12.columns#search-social
    .left
      - # Search Form goes here

%section.row
  %article.small-12.columns
    %h1="#{@current_state_name}, US"
    .row
      - @cities.each_slice(100) do |cities|
        .small-12.medium-4.columns
          %ul.no-bullet.list
            - cities.each do |city|
              %li.column
                %strong= city_state_name(city)
